{
    "name": "simexis/image-optimizer",
    "description": "Easily optimize images using PHP",
    "keywords": [
        "simexis",
        "image-optimizer"
    ],
    "homepage": "https://github.com/simexis/image-optimizer",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@simexis.be",
            "homepage": "https://simexis.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/process": "^2.8|^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2|^7.0",
        "symfony/var-dumper": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Simexis\\ImageOptimizer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Simexis\\ImageOptimizer\\Test\\": "tests"
        },
        "files": [
            "tests/helpers.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
